/* ======== new metabolicMine ======== */
body { background:url("../themes/metabolic/img/top-gradient.jpg") repeat-x top; /*font-family:"Myriad Pro", Arial, Garuda, Helvetica, Tahoma, sans-serif; color:#404040;*/ }

div.container h1, h2, h3 { font-family:"Myriad Pro", Arial, Garuda, Helvetica, Tahoma, sans-serif; color:#4d5154; }
div.container h2 { font-size:27px; margin-bottom:0; }
div.container h3 { font-size:22px; margin-bottom:0; }
div.container h4 { color:#FB641E; font-size:14px; margin-bottom:6px; }
    h4 span { font-family:"Times New Roman", "Times Roman", serif; font-style:italic; letter-spacing:-1px; font-size:11px; }
div.container p { color:#4d5154; margin-bottom:14px; }

/*a { color:#404040; }*/
div.container a:hover { color:#000000; }

/* ======== button love ======== */

div.container a.button, div.container a.button div, div.container a.button div span { font-size:13px; text-transform:uppercase;
  font-family:"Myriad Pro", Arial, Garuda, Helvetica, Tahoma, sans-serif; text-decoration:none; font-weight:bold;
  background-repeat:no-repeat; }
  div.container a.button { ; }
  div.container a.button:hover { text-decoration:underline; }
  div.container a.button div { background-position:top left; padding-left:18px; height:30px; position:relative; cursor:pointer; }
  div.container a.button div span { background-position:top right; display:block; height:30px; padding:6px 18px 0 0; cursor:pointer; }

/* default violet button link */
div.container a.button.violet { color:#FFF; }
div.container a.button.violet div { background-image:url("../themes/metabolic/buttons/btn-violet-left.png"); }
div.container a.button.violet span { background-image:url("../themes/metabolic/buttons/btn-violet-right.png"); }

/* default green button link */
div.container a.button.green { color:#FFF; }
div.container a.button.green div { background-image:url("../themes/metabolic/buttons/btn-green-left.png"); }
div.container a.button.green span { background-image:url("../themes/metabolic/buttons/btn-green-right.png"); }

/* default blue button link */
div.container a.button.blue { color:#FFF; }
div.container a.button.blue div { background-image:url("../themes/metabolic/buttons/btn-blue-left.png"); }
div.container a.button.blue span { background-image:url("../themes/metabolic/buttons/btn-blue-right.png"); }

/* gray button link */
div.container a.button.gray { color:#505050; }
div.container a.button.gray div { background-image:url("../themes/metabolic/buttons/btn-gray-left.png"); }
div.container a.button.gray span { background-image:url("../themes/metabolic/buttons/btn-gray-right.png"); }

div.container input.button { font-family:"Myriad Pro", Arial, Garuda, Helvetica, Tahoma, sans-serif; font-size:13px; height:30px;
  /* Opera 9 does not uppercase input elements */ text-transform:uppercase; font-weight:bold; border:none; cursor:pointer; }

