#search-box #magnifier { background:url("../themes/metabolic/icons/search-and-shade.png") no-repeat 64px 14px; line-height:100px; }

#search-box h3 { margin-left:6px; margin-bottom:0; }
#search-box p { margin-right:16px; font-size:13px; }

form#search div.input { background:url("../themes/metabolic/img/search-box-wide.png") no-repeat top; margin-top:10px; height:36px; }
form#search input.input { border:none; background:none; width:284px; padding:4px 20px 4px 20px; margin-bottom:10px;
  /* IE7 */ *margin-bottom:-10px; /*line-height:34px;*/ }
form#search input.input.eg { color:#888; }

#search-box input.orange { background:url("../themes/metabolic/buttons/orange-button-on-yellow.png") no-repeat bottom; color:#FFFFFF;
    padding:4px 40px; /* IE7 */ *padding:4px 20px; float:right; }
#search-box strong { color:#FB4F00; }

div.half-green div.top { background:url("../themes/metabolic/img/box-half-green-top.png") no-repeat top; height:5px; width:488px;
    font-size:0; line-height:0; overflow:hidden; margin-top:20px; }
div.half-green div.center { background:url("../themes/metabolic/img/box-half-green-center.png") repeat-y scroll 0 0 transparent;
    padding:10px 0; position:relative; min-height:210px; }
div.half-green div.bottom { background:url("../themes/metabolic/img/box-half-green-bottom.png") no-repeat bottom; width:488px;
    height:5px; font-size:0; line-height:0; overflow:hidden; }

div.half-violet div.top { background:url("../themes/metabolic/img/box-half-violet-top.png") no-repeat top; height:5px; width:488px;
    font-size:0; line-height:0; overflow:hidden; margin-top:20px; }
div.half-violet div.center { background:url("../themes/metabolic/img/box-half-violet-center.png") repeat-y scroll 0 0 transparent;
    padding:10px 0; position:relative; min-height:210px; }
div.half-violet div.bottom { background:url("../themes/metabolic/img/box-half-violet-bottom.png") no-repeat bottom; width:488px;
    height:5px; font-size:0; line-height:0; overflow:hidden; }

div.half-green div.text div.image, div.half-violet div.text div.image { float:left; padding:0 8px 20px 16px;
  background:url("../themes/metabolic/icons/shade.png") no-repeat 18px 62px; }
div.half-green p, div.half-violet p { padding-left:16px; }
div.half-green div.thumb, div.half-violet div.thumb { background:url("../themes/metabolic/thumbs/small-browser-thumb.png") no-repeat 252px 2px; }
    div.half-green div.thumb, div.half-violet div.thumb img { float:none; padding-top:18px; padding-bottom:20px; }

div.half-green h3 { color:#009900; margin-bottom:12px; }
div.half-green strong { color:#009900; }

div.half-green a.button.green { float:right; }

div.half-violet h3 { color:#8047d1; margin-bottom:12px; }
div.half-violet strong { color:#8047d1; }

div.half-violet a.button.violet { float:right; }

div.wide-gray div.top { background:url("../themes/metabolic/img/box-wide-gray-top.png") no-repeat top; height:5px; width:1000px;
    font-size:0; line-height:0; overflow:hidden; margin-top:20px; }
div.wide-gray div.center { background:url("../themes/metabolic/img/box-wide-gray-center.png") repeat-y scroll 0 0 transparent;
    padding:14px 0; position:relative; }
div.wide-gray div.bottom { background:url("../themes/metabolic/img/box-wide-gray-bottom.png") no-repeat bottom; width:1000px;
    height:5px; font-size:0; line-height:0; overflow:hidden; }

#actions { position:relative; }
#actions a.more { background:url("../themes/metabolic/icons/more-corner.png") no-repeat top; font-size:0; height:58px; line-height:0;
    overflow:hidden; position:absolute; bottom:-2px; right:-1px; width:54px; z-index:10; padding:0; cursor:pointer; }

#actions div.search, #actions div.lists, #actions div.templates { position:relative; margin-right:0; width:326px; /* override framework */ }
#actions div.search p, #actions div.lists p, #actions div.templates p { padding-right:0; }

#actions h3 { color:#FFFFFF; padding:29px 1px 7px 64px; margin:0 16px 10px 26px; line-height:26px; height:52px;
  position:absolute; top:0px; left:0px; width:71%; }
#actions div.image { float:left; background:url("../themes/metabolic/icons/shade.png") no-repeat bottom; float:left; margin-top:6px;
  margin-left:14px; z-index:10; position:relative; padding-bottom:18px; }
#actions p { padding:0 16px 0 24px; margin:20px 0 13px 0; font-size:13px; }
#actions h3 a { color:#FFF; text-decoration:none; }

#actions div.search h3 { background:#FB641E; }
#actions div.search h3 a, #actions div.search h3 a:visited { color:#FFF; }
#actions div.search strong { color:#FB4F00; }
#actions div.search input.orange-gray { background:url("../themes/metabolic/buttons/orange-button-on-gray.png") no-repeat bottom; color:#FFFFFF;
    padding:2px 40px; /* IE7 */ *padding:4px 20px; float:right; margin-right:30%; /* IE7 */ *margin-right:25%; }

#actions div.search div.input { background:url("../themes/metabolic/img/search-box-medium.png") no-repeat top; margin:0px 0px 20px 8px;
  height:36px; color:#888; }
#actionsInput.eg { color:#888; }
#actions div.search input.input { border:none; background:none; width:255px; padding:6px 20px 6px 30px; }
#actions div.search a, #actions div.search a:visited { color:#FB4F00; }

#actions div.lists h3 { background:#20A900; }
#actions div.lists strong { color:#009900; }

#actions div.lists a.button.green { float:right; margin-right:35%; }
#actions div.lists a.button.green, x:-moz-any-link { margin-right:65%; } /* FF2 */
#actions div.lists a.button.green, x:-moz-any-link, x:default { margin-right:35%; }  /* restore for FF3 */

#actions div.templates h3 { background:#8A54D3; }
#actions div.templates strong { color:#8047d1; }

#actions div.templates a.button.violet { float:right; margin-right:30%; /* IE7 */ *margin-right:35%; }
#actions div.templates a.button.violet, x:-moz-any-link { margin-right:50%; } /* FF2 */
#actions div.templates a.button.violet, x:-moz-any-link, x:default { margin-right:30%; }  /* restore for FF3 */

#more-actions div.action-1, #actions div.action-2, #actions div.action-3 { position:relative; }

#more-actions h3 { color:#FFFFFF; padding:18px 10px 18px 66px; margin:0 16px 10px 26px; line-height:26px; height:52px;
  position:absolute; top:0px; left:0px; width:63%; }
#more-actions img { float:left; margin-top:6px; margin-left:10px; z-index:10; position:relative; }
#more-actions p { padding:0 16px 0 24px; margin:25px 0 15px 0; }

#more-actions div.action-1 h3 { background:#DED580; }
#more-actions div.action-2 h3 { background:#5FD2CC; }
#more-actions div.action-3 h3 { background:#C960B2; }

div#api, div#rss, div#testimonials { font-size:11px; }
  div#api h4, div#rss h4, div#testimonials h4 { margin-top:6px; }
    div#api img, div#rss img, div#testimonials img { float:left; padding-right:8px; padding-bottom:40px; }
    div#api p, div#rss p, div#testimonials p { padding-right:28px; margin-bottom:4px; line-height:18px; }

div#api ul { list-style:none; margin:0; }

#articles { margin-bottom:0; }
    #articles td { padding:0 0 6px 0; }
    #articles td { padding-right:30px; }
        #articles td.date { padding-right:16px; }
    #articles .date a { text-decoration:none; font-weight:bold; font-size:18px; line-height:14px; }
        #articles .date a span { font-size:12px; font-weight:normal; }

#testimonials q { color:#0891c1; font-style:italic; }

