<aspects>
  <aspect name="Genomics">
    <subtitle>Genome annotation</subtitle>
    <icon-image>model/images/genomics.gif</icon-image>
    <large-image>model/images/genomics.gif</large-image>
    <intro-text>
      The gene structure and other genome annotation in pharMine are provided by
      a variety of curated source databases.  In most cases pharMine includes
      all genome features that have chromosomal locations.
    </intro-text>
    <tile-name>model/genomics.jsp</tile-name>
    <aspect-source name="Enseml" url="http://www.ensembl.org/"/>
  </aspect>

  <aspect name="Proteins">
    <subtitle>Protein and proteomics&lt;br/&gt;data</subtitle>
    <icon-image>model/images/proteins.png</icon-image>
    <large-image>model/images/proteins.png</large-image>
    <intro-text>
      Protein annotation (including family and domain annotation) from UniProt.
    </intro-text>
    <tile-name>model/proteins.jsp</tile-name>
    <aspect-source name="UniProt" url="http://www.uniprot.org"/>
  </aspect>

  <aspect name="Comparative Genomics">
    <subtitle>Orthologues and paralogues</subtitle>
    <icon-image>model/images/comparativeGenomics.png</icon-image>
    <large-image>model/images/comparativeGenomics.png</large-image>
    <intro-text>
      Orthologues and Paralogues.
    </intro-text>
    <tile-name>model/inparanoid.jsp</tile-name>
    <aspect-source name="Ensembl Compara" url="http://www.ensembl.org"/>
  </aspect>

  <aspect name="Interactions">
    <subtitle>IntAct</subtitle>
    <icon-image>model/images/interaction.gif</icon-image>
    <large-image>model/images/interaction.gif</large-image>
    <tile-name></tile-name>
    <intro-text>
      Protein-protein interactions from IntAct (PSI-MI format) and genetic interactions from the BioGRID.
    </intro-text>
    <tile-name>model/proteinInteraction.jsp</tile-name>
    <aspect-source name="IntAct" url="http://www.ebi.ac.uk/intact/"/>
    <aspect-source name="BioGRID" url="http://www.thebiogrid.org/"/>
  </aspect>

  <aspect name="Gene Ontology">
    <subtitle></subtitle>
    <icon-image>model/images/geneOntology.png</icon-image>
    <large-image>model/images/geneOntology.png</large-image>
    <intro-text>
      Gene Ontology (GO) annotations for &lt;i&gt;Homo Sapiens&lt;/i&gt;.  The GO annotations are 'attached' to Genes and so each Gene has a reference (link) to a list of its GO annotations (if available).
    </intro-text>
    <tile-name>model/GO.jsp</tile-name>
    <aspect-source name="Gene Ontology Consortium" url="http://www.geneontology.org/"/>
  </aspect>

  <aspect name="Pathways">
    <subtitle></subtitle>
    <icon-image>model/images/pathways.png</icon-image>
    <large-image>model/images/pathways.png</large-image>
    <intro-text>
      Reactome and Pathway Commons data for &lt;i&gt;Homo Sapiens&lt;/i&gt;.
    </intro-text>
    <tile-name>model/pathways.jsp</tile-name>
  </aspect>

  <aspect name="Disease">
    <subtitle>Human diseases</subtitle>
    <icon-image>model/images/disease.png</icon-image>
    <large-image>model/images/disease.png</large-image>
    <intro-text>
      Human Diseases, sourced currently from HUGE.
    </intro-text>
    <tile-name>model/diseaseAspect.jsp</tile-name>
    <aspect-source name="HuGE" url="http://www.kazusa.or.jp/huge/"/>
  </aspect>

  <aspect name="Chemicals">
    <subtitle>Chemicals</subtitle>
    <intro-text>
      This covers all chemicals of Biological interest. pharMine
      incorporates data from Stitch and ChEBI to provide Chemical
      annotation as well as links between chemicals, and links to proteins
    </intro-text>
    <tile-name>model/diseaseAspect.jsp</tile-name>
  </aspect>

  <aspect name="Nomenclature">
    <subtitle>Nomenclature</subtitle>
    <intro-text>
      pharMine contains nomenclature data from a wide variety of sources including HGNC, uniprot and ensembl.
    </intro-text>
    <tile-name>model/diseaseAspect.jsp</tile-name>
  </aspect>

  <aspect name="Phenotypes">
    <subtitle>Mammalian Phenotypes</subtitle>
    <intro-text>
      pharMine contains Mammalian phenotype data from MGI-Jackson
    </intro-text>
    <tile-name>model/diseaseAspect.jsp</tile-name>
  </aspect>

</aspects>
