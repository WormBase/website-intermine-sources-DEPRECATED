<aspects>

 <aspect name="Genomics">
    <subtitle>Genome annotation</subtitle>
    <icon-image>model/images/genomics.gif</icon-image>
    <large-image>model/images/genomics.gif</large-image>
    <intro-text>
      The gene structure and other genome annotation in HeightMine are provided by
      a variety of source databases.
    </intro-text>
    <tile-name>model/genomics.jsp</tile-name>
    <aspect-source name="FlyBase" url="http://www.flybase.org"/>
    <aspect-source name="Ensembl" url="http://www.ensembl.org/Anopheles_gambiae/"/>
  </aspect>

  <aspect name="Comparative Genomics">
    <subtitle>Orthologues and paralogues</subtitle>
    <icon-image>model/images/comparativeGenomics.png</icon-image>
    <large-image>model/images/comparativeGenomics.png</large-image>
    <intro-text>
      Orthologues and Paralogues.
    </intro-text>
    <tile-name>model/inparanoid.jsp</tile-name>
    <aspect-source name="Inparanoid" url="http://inparanoid.sbc.su.se/cgi-bin/index.cgi"/>
  </aspect>

  <aspect name="Proteins">
    <subtitle>Protein and proteomics&lt;br/&gt;data</subtitle>
    <icon-image>model/images/proteins.png</icon-image>
    <large-image>model/images/proteins.png</large-image>
    <intro-text>
      Protein annotation (including family and domain annotation) from UniProt
      .
    </intro-text>
    <tile-name>model/proteins.jsp</tile-name>
    <starting-points>Protein ProteinFeature ProteinDomain ProteinFamily</starting-points>
    <aspect-source name="InterPro" url="http://www.ebi.ac.uk/interpro"/>
    <aspect-source name="UniProt" url="http://www.uniprot.org"/>
  </aspect>

  <aspect name="Protein Interactions">
    <subtitle>IntAct</subtitle>
    <icon-image>model/images/interaction.gif</icon-image>
    <large-image>model/images/interaction.gif</large-image>
    <tile-name></tile-name>
    <intro-text>
      Proteins formatted in the Proteomics Standards Initiative (PSI)
      molecular interaction (PSI MI) format can be mapped to the UniMine
      data model.
    </intro-text>
    <starting-points>
      Protein ProteinInteraction ProteinInteractionExperiment ProteinInteractionTerm
    </starting-points>
    <tile-name>model/proteinInteraction.jsp</tile-name>
    <aspect-source name="IntAct" url="http://www.ebi.ac.uk/intact/"/>
  </aspect>

  <aspect name="Genetic Interactions">
    <subtitle>IntAct</subtitle>
    <icon-image>model/images/interaction.gif</icon-image>
    <large-image>model/images/interaction.gif</large-image>
    <tile-name></tile-name>
    <intro-text>
      Genetic interactions from the BioGRID.
    </intro-text>
    <tile-name>model/geneticInteraction.jsp</tile-name>
    <aspect-source name="BioGRID" url="http://www.thebiogrid.org/"/>
  </aspect>


  <aspect name="Gene Ontology">
    <subtitle></subtitle>
    <icon-image>model/images/geneOntology.png</icon-image>
    <large-image>model/images/geneOntology.png</large-image>
    <intro-text>
      The Gene Ontology (GO) project is a collaborative effort to
      address the need for consistent descriptions of gene products in
      different databases.
&lt;p&gt;
      The GO collaborators are developing three structured, controlled
      vocabularies (ontologies) that describe gene products in terms of
      their associated biological processes, cellular components and
      molecular functions in a species-independent manner.
&lt;p&gt;
    </intro-text>
    <starting-points>Gene GOTerm GOAnnotation</starting-points>
    <tile-name>model/GO.jsp</tile-name>
    <aspect-source name="Gene Ontology Consortium" url="http://www.geneontology.org/"/>
  </aspect>

  <aspect name="Pathways">
    <subtitle></subtitle>
    <icon-image>model/images/pathways.png</icon-image>
    <large-image>model/images/pathways.png</large-image>
    <intro-text>
       KEGG pathway data for &lt;i&gt;Drosophila melanogaster&lt;/i&gt;.
    </intro-text>
    <tile-name>model/pathways.jsp</tile-name>
    <aspect-source name="Kegg pathway database" url="http://www.genome.jp/kegg/pathway.html"/>
  </aspect>

  <aspect name="Disease">
    <subtitle>Human disease matches&lt;br/&gt;from OMIM</subtitle>
    <icon-image>model/images/disease.png</icon-image>
    <large-image>model/images/disease.png</large-image>
    <intro-text>
      OMIM Disease.
    </intro-text>
    <tile-name>model/diseaseAspect.jsp</tile-name>
    <aspect-source name="OMIM-Disease" url="http://www.ncbi.nlm.nih.gov/sites/entrez?db=omim"/>
  </aspect>


  <aspect name="Literature">
    <subtitle></subtitle>
    <icon-image>model/images/book.png</icon-image>
    <large-image>model/images/book.png</large-image>
    <intro-text>
      A mapping between genes and publications.
    </intro-text>
    <tile-name>model/literature.jsp</tile-name>
    <aspect-source name="NCBI" url="http://www.ncbi.nlm.nih.gov/"/>
  </aspect>

  <aspect name="Height">
    <subtitle>Height data</subtitle>
    <icon-image>model/images/height.png</icon-image>
    <large-image>model/images/height.png</large-image>
    <intro-text>
       Height-data
    </intro-text>
    <tile-name>model/height.jsp</tile-name>
  </aspect>

  <aspect name="Haem-Atlas">
    <subtitle>Haem-Atlas-Data</subtitle>
    <icon-image>model/images/book.jpg</icon-image>
    <large-image>model/images/book.jpg</large-image>
    <intro-text>
       haem-atlas-data
    </intro-text>
    <tile-name>model/heam.jsp</tile-name>
  </aspect>


</aspects>
