/* background */
body { background:url("img/top-gradient.jpg") repeat-x scroll center top transparent; }

/* quick search */
input#quickSearchInput { background:url("img/quick-search-alt-bg.png") no-repeat top; border:none; padding:5px 2px 0 6px;
  font-size:12px; margin-top:1px; }
#submenudiv #quicksearch { color:#4B3F4B; }

#pagecontentcontainer { background:#FEFEFE; }

/* main boxes */
div.gradientbox { border:1px solid #E3D85A; background:/*#FFFEF2*/url("img/yellow-bg.jpg") repeat-x top; }
div.plainbox { border:1px solid #BEDCE4 !important; background:#E6F7FE; }

/* box filter bars */
div.filterBar { background:#FFFCD9; border-color:#E3D85A; }
div#filter_tool_bar { border-color:#E3D85A; }

div .gradientbox h1, div .plainbox h1 { color:#4D5154; font-size:22px; }

/* bagDetails.do textarea edit */
#bagDescriptionDiv { cursor:pointer; }

/* bagDetails.do sidebar */
#convertList.listtoolbox, #linkOuts.listtoolbox { padding:0; border:1px solid #D2D2D2; }
  #convertList.listtoolbox h3, #linkOuts.listtoolbox h3 { padding:4px 10px; margin:0; margin-bottom:10px; background:#EEE; }
  #convertList.listtoolbox p { margin-bottom:10px; }

/* lists results page when logged in */
#listTags { background-color: #E6F7FE; border:1px solid #BEDCE4; margin-left: 10px; padding: 3px 10px; }

/* further information */
div.geneInformation table { border-width:1px 0 1px 1px; border-style:solid; border-color:#DFDFDF; margin-bottom:10px; }
  div.geneInformation table tr.odd td { background:#E6F7FE; }
    div.geneInformation table td { border-right:1px solid #DFDFDF; padding:4px; }
    div.geneInformation table th { background:#F5F5F5; border-right:1px solid #DFDFDF; padding:4px; color:#4B3F4B; }
    div.geneInformation table th.type { width:10%; }
    div.geneInformation table th.comment { width:100%; }
div.geneInformation h3 { color:#008000; font-size:13px; margin:0; margin-bottom:10px; }
  div.geneInformation h3.uniprot { background:url('icons/uniprot.png') no-repeat right; padding:16px 28px 16px 0; color:#4896B2;
  display:inline; }
  div.geneInformation h3.overlapping { color:#4896B2; }
  div.geneInformation h4 { background:#F5F5F5; border:1px solid #DFDFDF; padding:4px; }

form#saveFromIdsToBagForm input.submit { background-image:url("../../images/buttons/button-bg-lists.gif"); background-repeat:repeat-none;
  background-position:left top; border:1px outset #CCCCCC; cursor:pointer; height:23px; padding:1px 2px 1px 32px; vertical-align:middle;
  color:#101010; }

/* lists analysis (bagDetails.do) */
div.heading.results { border:1px solid #BEDCE4; background:#E6F7FE; margin-left:0; }
div.heading.results h1 { color:#4D5154; font-size:22px; margin-bottom:10px; }
div.heading.results #tool_bar_div { float:left; background:#E6F7FE !important; }
div.heading.results #tool_bar_div li { border:1px solid #D2D2D2; background:#EEEEEE; margin-right:10px; padding:4px; font-weight:normal; }

/* list results page */
div.resultsPage table.results { border:1px solid #DFDFDF; border-left:0; margin-bottom:10px; width:93.5%; }
  div.resultsPage table.results a { color:#1F7492; }
  div.resultsPage table.results th { border:none; border-left:1px solid #DFDFDF !important; font-size:16px; background:#F5F5F5; color:#4B3F4B; }
  div.resultsPage table.results tr.odd td, div.table table.results tr.even td { border:none; border-left:1px solid #DFDFDF !important; }
  div.resultsPage table.results tr.even td { background:#E6F7FE; border-top:1px solid #C1DAD7; border-bottom:1px solid #C1DAD7; }
  div.resultsPage table.results tr.odd td.highlightCell, div.table table.results tr.odd td.highlightObject { background:#FFFBD8; }
  div.resultsPage table.results tr.even td.highlightCell, div.table table.results tr.even td.highlightObject { background:#FFFBD8;
    border-top:1px solid #E3D85A; border-bottom:1px solid #E3D85A; }
  div.resultsPage table.results tfoot tr td { background:#FFF; color:#333; }
  div.resultsPage table.results input { vertical-align:middle; }
  div.resultsPage table.results b { padding-right:3px; }

#convertList.bochs form#bagDetailsForm { margin-bottom:0 !important; } /* why the */

#tool_bar_div { background:none; }
  #tool_bar_div ul#button_bar li.tb_button { cursor:pointer; border:outset 1px #CCCCCC; background-image:url("../../images/buttons/button-bg.gif");
  background-position:left top; background-color:#999999; background-repeat:repeat-x; vertical-align:middle; margin-right:2px; }

