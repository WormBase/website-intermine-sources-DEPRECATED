/* bochs theme */
div.gradientbox { border:1px solid #DFDFDF; background:url("gray_grad.png") repeat-x scroll 0 0 transparent; }
div.gradientbox a, div.gradientbox a:hover { color:#1F7492 !important; }

div.filterBar { border:none !important; background:none !important; }
#filter_tool_bar { border:none !important; border-bottom:1px solid #DFDFDF !important; }
  #filter_tool_bar strong { color:#333; }

#ws_all_templates_template a.anchor { background:url("../../images/icons/go-top-16.png") no-repeat left; padding-left:20px; line-height:40px; }
  #ws_all_templates_template a.anchor img { display:none; }

/* context help "?" */
a.contextHelpSmall { background:url("icons/help.png") no-repeat left; padding:6px 0 6px 25px !important; }
  p.alignright a.contextHelpSmall, p.navtrail a.contactUsLink, p.alignright a.contactTeam, p.alignright span.omgDivider { display:none; } /* hide context help in breadcrumbs */
  #navtrail p.alignright { margin:0 -10px 10px 0; }

/* contact team */
a.contactTeam { background:url("icons/email.png") no-repeat left; padding:6px 0 6px 25px !important; }
span.omgDivider { border-right:1px solid #004499; margin-right:4px; }

/* submenu border */
ul#submenulist li div { border-left-color:#FEF9F3; }

/* login bar */
ul#loginbar { margin-top:10px !important; }
ul#loginbar li { border-right-color:#004499; }

/* topnav */
ul#topnav { font-size:0.7em; margin:0; position:absolute; right:0; top:0; white-space:nowrap; list-style:none; text-transform:capitalize; }
  ul#topnav.smallScreen { right:40px; }
  ul#topnav li { display:inline; border-right:1px solid #505050; padding:0 6px 0 3px; margin:0; }
    ul#topnav li.last { border:none; }
  ul#topnav a:link, ul#topnav a:visited { color:#505050; }

/* results table on objectDetails? */
table.results { border:1px solid #DFDFDF; border-left:0; margin-bottom:10px; width:93.5%; }
  table.results a { color:#1F7492; }
  table.results th { border:none; border-left:1px solid #DFDFDF !important; font-size:16px; background:#F5F5F5; color:#4B3F4B; }
  table.results tr.odd td, div.table table.results tr.even td { border:none; border-left:1px solid #DFDFDF !important; }
  table.results tr.even td { background:#E6F7FE; border-top:1px solid #C1DAD7; border-bottom:1px solid #C1DAD7; }
  table.results tr.odd td.highlightCell, div.table table.results tr.odd td.highlightObject { background:#FFFBD8; }
  table.results tr.even td.highlightCell, div.table table.results tr.even td.highlightObject { background:#FFFBD8;
    border-top:1px solid #E3D85A; border-bottom:1px solid #E3D85A; }
  table.results tfoot tr td { background:#FFF; color:#333; }
  table.results input { vertical-align:middle; }
  table.results b { padding-right:3px; }

/* external links on objectDetails (and bagDetails!!!) */
div.externalLinks { ; }
  div.externalLinks h3 { color:#4896B2; margin-bottom:4px; font-size:13px; }
  div.externalLinks ul { list-style:none; }
    div.externalLinks ul li { display:inline; margin:0; margin-right:6px; float:left; }
      div.externalLinks ul li a { line-height:27px; height:27px; }
      div.externalLinks ul li a.ext_link { display:none; }
      div.externalLinks ul li a.ext_link.image { display:inline; /*background:url("../../images/icons/external-link-8.png") no-repeat top right;
        padding:34px 10px 0 0;*/ }
        div.externalLinks ul li a.ext_link.image img { background:url("../../images/icons/external-link-8.png") no-repeat top right; padding-right:10px; }
        div.externalLinks ul li a span { float:left; display:inline; font-size:9px; line-height:20px; }
#externalLinksClear { clear:both; margin-bottom:10px; }

/* object details cytoscape (override webapp) */
h3.interactions { color:#4896B2; font-size:13px; margin:10px 0 10px; }

/* object details further information tables */
table.results tr th.columnHeader { font-size:16px; background:#F5F5F5 !important; color:#4B3F4B !important; }

/* submenu */
#submenufixed, #submenu { background-image:url('img/submenu-bottom-border.png'); background-repeat:repeat-x; background-position:bottom; }
#submenufixed ul#submenulist li div, #submenu ul#submenulist li div { text-transform:uppercase; }
#submenufixed ul#submenulist li div a, #submenu ul#submenulist li div a { text-decoration:underline; }

/* handwritten messages */
#pagecontentcontainer.bag-page p#contactUsLink, #pagecontentcontainer.bagupload-page p#contactUsLink,
  #pagecontentcontainer.bagview-page p#contactUsLink { display:none; }
#pagecontentcontainer.bagupload-page #pagecontent, #pagecontentcontainer.bag-page #pagecontent { background-image:url('img/arrowtip-view-lists.png');
  background-repeat:no-repeat; background-position:45px 10px; }
#pagecontentcontainer.bagview-page #pagecontent { background-image:url('img/arrowtip-upload-list.png'); background-repeat:no-repeat;
  background-position:16px 6px; }
#pagecontentcontainer.bagview-page #pagecontent div.body { margin-top:40px; }