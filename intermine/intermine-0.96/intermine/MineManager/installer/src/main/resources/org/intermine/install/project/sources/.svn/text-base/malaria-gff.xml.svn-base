<?xml version="1.0" encoding="UTF-8" ?>
<sd:source xmlns:sd="http://flymine.org/sourcedescriptor/1.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://flymine.org/sourcedescriptor/1.0 http://www.flymine.org/download/schemas/sourcedescriptor.xsd"
           type="malaria-gff">

    <property name="gff3.taxonId">
        <validation><![CDATA[^\d+(\s\d+)*$]]></validation>
    </property>
    <property name="gff3.seqClsName" />
    <property name="gff3.dataSourceName" />
    <property name="gff3.seqDataSourceName" />
    <property name="gff3.dataSetTitle" />
    <property name="src.data.dir" required="true" type="directory"/>

    <derivation antTask="have.file.gff3" />
    
</sd:source>
