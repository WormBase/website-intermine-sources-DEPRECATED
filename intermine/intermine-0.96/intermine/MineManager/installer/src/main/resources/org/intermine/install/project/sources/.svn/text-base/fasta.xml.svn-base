<?xml version="1.0" encoding="UTF-8" ?>
<sd:source xmlns:sd="http://flymine.org/sourcedescriptor/1.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://flymine.org/sourcedescriptor/1.0 http://www.flymine.org/download/schemas/sourcedescriptor.xsd"
           type="fasta">

    <property name="fasta.taxonId" required="true">
        <validation><![CDATA[^\d+(\s\d+)*$]]></validation>
    </property>
    <property name="fasta.className" type="class" />
    <property name="fasta.classAttribute" />
    <property name="fasta.dataSetTitle" />
    <property name="fasta.dataSourceName" />
    <property name="src.data.dir" required="true" type="directory"/>
    <property name="fasta.includes" />
    <property name="fasta.sequenceType" />
    <property name="fasta.loaderClassName" type="class" />

</sd:source>
