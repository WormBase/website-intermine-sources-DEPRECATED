compile.dependencies = intermine/objectstore/main, \
           bio/core/main, \
           intermine/integrate/main

have.file.obo = true

# specify the name of the ontology
obo.ontology.name = Worm Development

# set a URL at which more information on the ontology can be found
obo.ontology.url = http://www.obofoundry.org/cgi-bin/detail.cgi?id=worm_development

# set the class name for terms that will be created, you may need to define
# this class in a worm-development-ontology_additions.xml file for this source
obo.term.class = DevelopmentTerm

