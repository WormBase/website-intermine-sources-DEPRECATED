#wrapper { margin-top:10px; }

/* webapp.css fix */
li { margin-left:auto; }
#ctxHelpDiv.welcome { margin-top:10px; }

/* faux welcome */
#ctxHelpDiv.welcome div.topBar.info { margin:0 !important; }
#ctxHelpDiv.welcome div.topBar.info a { background:url("../themes/metabolic/icons/toggle-expand.png") no-repeat left;
  padding-left:20px; }

/* welcome */
#welcome { position:relative; background:#E6F7FE; border:1px solid #BEDCE4; padding:10px 0; }
  #welcome p { font-size:14px; line-height:24px; }

  #welcome #welcome-content ul { font-size:14px; margin:0 0 10px 14px; list-style-type:square; }
  #welcome #welcome-content h2 { color:#0891C1; }

  #welcome ul#switcher { list-style:none; float:right; margin:10px 10px 0 0; }
    #welcome ul#switcher li { float:left; padding-right:6px; font-size:12px; }
      #welcome ul#switcher a { text-decoration:none; border:1px solid #BEDCE4; padding:0 8px; }
      #welcome ul#switcher li.current a { background:#BFEBFE; }

  #welcome #thumb { margin:0; background:url("../themes/metabolic/thumbs/welcome-thumb.png") no-repeat top; height:205px; }
      #welcome #thumb img { margin:2px 36px; }
  #welcome a.close { background:url("../themes/metabolic/icons/toggle-minus.png") no-repeat top; height:16px; width:16px; font-size:0;
      line-height:0; position:absolute; right:6px; top:6px; z-index:1000; }

/* actions */
#search-bochs, #genomic-bochs, #upload-bochs, #lists-bochs, #templates-bochs { line-height:19px; margin-top:20px; position:relative; }
#search-bochs h3, #genomic-bochs h3, #upload-bochs h3, #lists-bochs h3, #templates-bochs h3 { padding:10px 0 10px 10px; }
#search-bochs .text, #lists-bochs .text, #genomic-bochs .text, #upload-bochs .text, #templates-bochs .text { padding:10px; font-size:14px; }
#search-bochs h3 a, #lists-bochs h3 a, #genomic-bochs h3 a, #upload-bochs h3 a, #templates-bochs h3 a { color:#000; text-decoration:none; }

#search-bochs { border:1px solid #FBC9B2; background:#FBF4F1; height:240px; }
  #search-bochs h3 { background:#FBC9B2; padding-left:85px; }
  #search-bochs input#dataSearch { width:279px; }
  #search-bochs input#dataSearch.eg { color:#666; }
  #search-bochs input#exptSearch { width:200px; }
  #search-bochs input#exptSearch.eg { color:#666; }
  #search-bochs img.title { position:absolute; top:10px; left:10px; }

#genomic-bochs { border:1px solid #192F39; height:180px; }
  #genomic-bochs h3 { background:#192F39; padding-left:85px; }
    #genomic-bochs h3 a { color:#FFF; }
  #genomic-bochs img.title { position:absolute; top:10px; left:10px; }

#upload-bochs { border:1px solid #63B7DE; height:275px; }
  #upload-bochs h3 { background:#63B7DE; padding-left:80px; }
  #upload-bochs img.title { position:absolute; top:10px; left:10px; }
  #upload-bochs select { display:block; width:278px; margin-bottom:4px; }
  #upload-bochs textarea { width:273px; }

#templates-bochs { border:1px solid #E8D9FF; height:275px; }
  #templates-bochs h3 { background:#E8D9FF; padding-left:85px; }
  #templates-bochs img.title { position:absolute; top:10px; left:10px; }

#lists-bochs { border:1px solid #D0FCC5; height:275px; }
  #lists-bochs h3 { background:#D0FCC5; padding-left:90px; }
  #lists-bochs img.title { position:absolute; top:10px; left:10px; }

/* 3 boxes */
#bochs { border:1px solid #BEDCE4; background:#FCFEFF; margin-top:20px; height:284px; width:299px; }
  #bochs.last { margin-right:0; }
  #bochs h3 { padding:10px 0 10px 10px; color:#0891C1; }
  #bochs a.heatmap { text-decoration:none; color:#000; font-weight:bold; }
    #bochs a.heatmap img { vertical-align:middle; margin:0 20px 10px 0; }
    #bochs a.heatmap span { display:inline-block; width:118px; vertical-align:middle; }
  #bochs .text { padding:10px; font-size:14px; }
    #bochs .text p { margin-bottom:10px; }

img { margin-left:10px; }