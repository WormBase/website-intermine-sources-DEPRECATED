/* =TD £TABLE */
td.resBar {
    background-image:url('../theme/res_bar_middle.gif');}

/* =DIV £OBJECTDETAILSTEMPLATETABLE
-------------------------------------- */
#summary {
  top:360px; left:40%; position:absolute;
  min-width:100px; padding:5px;
  background-color:#fff; border: solid black 1px;}

/* template results - used by object & bag details pages */
/* =TABLE */
table.refSummary {
  width: 100%;
  border-top: 1px solid #888; border-left: 1px solid #888;}

/* =TD */
table.refSummary thead tr td {
  background-color: #eee; border-bottom: 1px solid #888; border-right: 1px solid #888;}

/* =TD */
table.refSummary thead tr td.object {
  background-color: #eee; font-weight: bold; border-bottom: 1px solid #888; border-right: 1px solid #888;}

/* =TD */
table.refSummary tbody tr td {
  padding: 3px;
  background-color: #F5FFF3; border-bottom: 1px solid #888; border-right: 1px solid #888;}

/* =SPAN */
span.templateResCount {
  color: #555; white-space: nowrap; font-size: 10px;}

/* =DIV */
div.templateIcon {
  float: left; padding-top: 2px;}
  
div.resultsPage {
  clear:both; padding-top: 10px;
}
	
/* =TABLE £TABLE,£BAGDETAILS*/
table.results {
  clear:left;
  border-left: solid 1px #666; border-top: solid 1px #666;font-size: small;}

/* =TD, =TH £TABLE,£BAGDETAILS*/
table.results th, table.results td {
  padding: 4px; border-right: solid 1px #666; border-bottom: solid 1px #666;
  background: #fff;}

/* =TH £TABLE,£BAGDETAILS*/
table.results th {
  background: #eee;font-size: 12px;}

/* =TD £TABLE,£BAGDETAILS*/
table.results td td {
  border: none;}

/* =TD £TABLE,£BAGDETAILS*/
table.results tr.even TD{
  background-color: #F5FFF3;}

/* =TD, TH £TABLE,£BAGDETAILS*/
table.results th.checkbox,table.results td.checkbox{
  border-right-style: dotted;}
  
table.results tr td.highlightCell { 
	background-color: #BAECBE; 
} 
  
