compile.dependencies = intermine/objectstore/main,\
                       intermine/integrate/main,\
                       intermine/objectstore/model/testmodel,\
                       intermine/integrate/test,\
                       intermine/objectstore/test
### intermine/objectstore/test is required for testmodel_data.xml et al
### intermine/integrate/test is required for testsource_keys.properties

objectstore.name = os.unittest
db.name = db.unittest
model.name = testmodel

# choose the intermine.properties file from $HOME:
intermine.properties.file = testmodel.properties
default.intermine.properties.file = ../../intermine/default.intermine.properties
