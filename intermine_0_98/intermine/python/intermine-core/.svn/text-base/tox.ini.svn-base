[tox]
envlist = 
   py25, py26, py27, pypy, jython

[testenv]
commands = 
    python setup.py test
    python tests/live_lists.py

[testenv:py25]
deps = 
    simplejson

[testenv:py24]
deps = 
    simplejson

[testenv:jython]
basepython=jython
deps = 
    simplejson
commands = 
    jython tests/live_lists.py
