div.imbedded-table-tooltip {
    border-width:       2px;
    width:              500px;
    border-style:       solid;
    border-color:       white;
    padding:            5px;
    color:              white;
    background:         #666;
    opacity:            0.90;
    font-family: "Droid Serif", "Neuton", "Georgia", "Times", "serif";
    font-weight: bold;
}

.imbedded-count-displayer {
    margin-left: 10px;
    margin-right: 10px;
}

.imbedded-table-container {
    margin: 20px;
    border-style: solid;
    border-width: 2px;
    width: 80%;
    border-color: #365252;
    padding-bottom: 5px;
}

.imbedded-scroll-container {
    overflow: auto;
}

.imbedded-table-titlebox {
    font-size: 24px;
    padding: 20px;
    color: black;
    padding: 5px;
}

.imbedded-table-title {
    font-family: "Droid Serif", "Neuton", "Georgia", "Times", "serif";
    color: #444;
}

.imbedded-column-header-row {
    font-family: "Droid Serif", "Neuton", "Georgia", "Times", "serif";
    font-weight: bold;
    background: #666;
    color: white;
    vertical-align: top;
}

.imbedded-table {
    border-collapse: collapse;
    width: 100%;
    overflow: scroll;
}

.imbedded-cell {
    font-size: 14px;
    font-family: "Droid Serif", "Neuton", "Georgia", "Times", "serif";
    padding: 4px;
}

.imbedded-cell > a {
    color: #444;
}

.imbedded-data-row {
    color: #444;
}

.imbedded-cell > a:hover {
    color: black;
    font-style: italic;
}

.imbedded-row-odd {
    background: #D5E8F3;
}

.imbedded-row-even {
    background: white;
}

.imbedded-exportlink, .imbedded-pagelink {
    font-family: "Droid Serif", "Neuton", "Georgia", "Times", "serif";
    text-decoration: none;
    color: black ! important;
    font-size: 12px;
    padding-right: 5px;
    padding-left: 5px;
}

.imbedded-addition-link, .imbedded-next {
    float: right;
}

.imbedded-table-container > a:hover {
    font-style: italic;
}

.imbedded-table-expand-help {
    color: #666;
    float: right;
    font-size: 50%;
}

.imbedded-null {
    opacity: 0.5;
}
